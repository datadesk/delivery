{
  "name": "@datagraphics/delivery",
  "version": "0.6.0",
  "type": "module",
  "description": "Push and pull assets to and from S3 the Data Graphics way.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build-types && npm run rollup-types && npm run bundle",
    "build-types": "tsc",
    "rollup-types": "api-extractor run --local && rm -rf temp",
    "bundle": "esbuild --bundle --minify --main-fields=module,main --legal-comments=none --platform=node --format=esm --outfile=lib/index.js src/index.ts",
    "build-docs": "documentation readme src/index.ts --parse-extension ts --section API --markdown-toc --config documentation.yml",
    "prepublishOnly": "npm run build && npm run build-docs",
    "release": "np --no-yarn --no-tests",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datadesk/delivery.git"
  },
  "keywords": [
    "s3",
    "aws",
    "deployment"
  ],
  "author": "Ryan Murphy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datadesk/delivery/issues"
  },
  "homepage": "https://github.com/datadesk/delivery#readme",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.15.0",
    "@datagraphics/prettier-config": "^2.0.0",
    "@microsoft/api-extractor": "^7.15.1",
    "@types/mime": "^2.0.2",
    "@types/node": "^14.14.37",
    "documentation": "^13.2.0",
    "esbuild": "^0.11.20",
    "fast-glob": "^3.2.5",
    "mime": "^2.5.2",
    "np": "^7.0.0",
    "prettier": "2.3.0",
    "typescript": "^4.2.4"
  },
  "prettier": "@datagraphics/prettier-config"
}
